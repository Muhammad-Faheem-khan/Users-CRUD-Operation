nents: {\r\n      PopUpBox,\r\n    },\r\n    computed:{\r\n        userList(){\r\n            return store.getters.getAllUsers\r\n        }\r\n    },\r\n    methods:{\r\n    editTask(id){\r\n      store.dispatch('updateUser', id)\r\n    },\r\n    deleteTask(id){\r\n      store.dispatch('deleteUser', id)\r\n    }\r\n    },\r\n    created(){\r\n        store.dispatch('userListUpdate')\r\n    }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.container{\r\n  border: 2px solid black;\r\n  border-radius: 0.5rem;\r\n  padding-left: 0;\r\n  width: 75%;\r\n  margin: 0 auto;\r\n  position: relative;\r\n}\r\n.heading{\r\n  font-size: 1.5rem;\r\n  text-align: center;\r\n}\r\n.usersData {\r\n  list-style-type: none;\r\n  padding: 1rem;\r\n  margin-top: 0.5rem;\r\n  display: flex;\r\n  justify-content: space-between;\r\n  margin-bottom: 1rem;\r\n}\r\n.usersData:hover {\r\n  background-color: #f2f6fc;\r\n  color: #18233b;\r\n}\r\n.blur {\r\n  filter: blur(2px);\r\n  z-index: 100;\r\n}\r\n.addUser-btn{\r\n  background-color: lightskyblue;\r\n  margin: 1rem;\r\n}\r\n.addbtn-container{\r\n  display: flex;\r\n  justify-content: flex-end;\r\n}\r\n.userInfo{\r\n  display: flex;\r\n  flex-direction: column;\r\n  text-align: center;\r\n}\r\n.data{\r\n  font-size: 1rem;\r\n  margin-right: 2rem;\r\n  margin-bottom: 0.2rem;\r\n  margin-top: 0.2rem;\r\n}\r\n.button{\r\n  padding: 0.4rem 0.5rem;\r\n  margin-right: 0.5rem;\r\n  font-size: 1rem;\r\n  font-weight: 600;\r\n  border: none;\r\n  border-radius: 0.4rem;\r\n}\r\n.edit-btn{\r\n  background-color: greenyellow;\r\n}\r\n.delete-btn{\r\n  background-color: red;\r\n}\r\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}